{
  "name": "@hayspec/cli",
  "version": "0.3.3",
  "description": "CLI for Hayspec framework.",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "bin": {
    "hayspec": "./bin/hayspec"
  },
  "scripts": {
    "clean": "rm -Rf ./dist",
    "transpile": "npm run clean; tsc",
    "lint": "tslint 'src/**/*.ts?(x)'",
    "prepublishOnly": "npm run transpile",
    "test": "npm run transpile && ava"
  },
  "ava": {
    "compileEnhancements": false,
    "extensions": [
      "ts"
    ],
    "require": [
      "ts-node/register"
    ],
    "files": [
      "src/tests/*.test.ts",
      "src/tests/**/*.test.ts"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hayspec/monorepo.git"
  },
  "bugs": {
    "url": "https://github.com/hayspec/monorepo/issues"
  },
  "homepage": "https://github.com/hayspec/monorepo#readme",
  "keywords": [
    "test",
    "testing",
    "spec",
    "specification",
    "hay",
    "javascript",
    "typescript",
    "nodejs",
    "tdd",
    "unit",
    "cli"
  ],
  "author": "Kristijan Sedlak (Xpepermint)",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^10.3.2",
    "@types/yargs": "^11.0.0",
    "ava": "^1.0.0-beta.6",
    "eslint": "4.19.1",
    "ts-node": "^6.1.0",
    "tslint": "^5.10.0",
    "typescript": "^2.9.1"
  },
  "dependencies": {
    "@hayspec/core": "0.3.3",
    "@hayspec/reporter": "0.3.3",
    "@hayspec/runner": "0.3.3",
    "yargs": "^11.0.0"
  }
}
