{
  "name": "@hayspec/reporter",
  "version": "0.2.1",
  "description": "Reporter for Hayspec framework.",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "clean": "rm -Rf ./dist",
    "transpile": "npm run clean; tsc",
    "lint": "tslint 'src/**/*.ts?(x)'",
    "prepublishOnly": "npm run transpile",
    "test": "ava"
  },
  "ava": {
    "compileEnhancements": false,
    "extensions": [
      "ts"
    ],
    "require": [
      "ts-node/register"
    ],
    "files": [
      "src/tests/*.test.ts",
      "src/tests/**/*.test.ts"
    ]
  },
  "license": "MIT",
  "devDependencies": {
    "@types/node": "10.3.3",
    "ava": "1.0.0-beta.6",
    "ts-node": "^6.1.0",
    "typescript": "^2.9.1"
  },
  "dependencies": {
    "@hayspec/core": "^0.2.1",
    "chalk": "^2.4.1"
  }
}
