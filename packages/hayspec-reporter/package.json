{
  "name": "@hayspec/reporter",
  "version": "0.4.19",
  "description": "Reporter for Hayspec framework.",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "clean": "rm -Rf ./dist",
    "transpile": "npm run clean; tsc",
    "prepare": "npm run transpile",
    "test": "nyc ava"
  },
  "ava": {
    "compileEnhancements": false,
    "extensions": [
      "ts"
    ],
    "require": [
      "ts-node/register"
    ],
    "files": [
      "src/tests/*.test.ts",
      "src/tests/**/*.test.ts"
    ]
  },
  "nyc": {
    "extension": [
      ".ts"
    ],
    "require": [
      "ts-node/register"
    ],
    "exclude": [
      "src/tests"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hayspec/monorepo.git"
  },
  "bugs": {
    "url": "https://github.com/hayspec/monorepo/issues"
  },
  "homepage": "https://github.com/hayspec/monorepo#readme",
  "keywords": [
    "test",
    "testing",
    "spec",
    "specification",
    "hay",
    "javascript",
    "typescript",
    "nodejs",
    "tdd",
    "unit",
    "cli"
  ],
  "author": "Kristijan Sedlak (Xpepermint)",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^10.3.2",
    "ava": "1.0.0-beta.6",
    "nyc": "^12.0.2",
    "ts-node": "^7.0.1",
    "typescript": "^3.0.1"
  },
  "dependencies": {
    "@hayspec/spec": "^0.4.19",
    "chalk": "^2.4.1"
  },
  "gitHead": "03535c698ebd35ec94c63869f18cdde2380f739f"
}
