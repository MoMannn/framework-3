{
  "name": "@hayspec/spec",
  "version": "0.7.6",
  "description": "Core logic for Hayspec framework.",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "clean": "rm -Rf ./dist",
    "transpile": "npm run clean; tsc",
    "prepare": "npm run transpile",
    "test": "nyc ava --verbose"
  },
  "ava": {
    "compileEnhancements": false,
    "extensions": [
      "ts"
    ],
    "require": [
      "ts-node/register"
    ],
    "files": [
      "src/tests/*.test.ts",
      "src/tests/**/*.test.ts"
    ]
  },
  "nyc": {
    "extension": [
      ".ts"
    ],
    "require": [
      "ts-node/register"
    ],
    "exclude": [
      "src/tests"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hayspec/framework.git"
  },
  "bugs": {
    "url": "https://github.com/hayspec/framework/issues"
  },
  "homepage": "https://github.com/hayspec/framework#readme",
  "keywords": [
    "test",
    "testing",
    "spec",
    "specification",
    "hay",
    "javascript",
    "typescript",
    "nodejs",
    "tdd",
    "unit",
    "cli"
  ],
  "author": "Kristijan Sedlak (Xpepermint)",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^10.11.4",
    "ava": "^1.0.0-beta.8",
    "nyc": "^13.0.1",
    "ts-node": "^7.0.1",
    "typescript": "^3.1.1"
  }
}
