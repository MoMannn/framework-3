{
  "name": "@hayspec/spec",
  "version": "0.4.14",
  "description": "Core logic for Hayspec framework.",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "clean": "rm -Rf ./dist",
    "transpile": "npm run clean; tsc",
    "lint": "tslint 'src/**/*.ts?(x)'",
    "prepublishOnly": "npm run transpile",
    "test": "ava"
  },
  "ava": {
    "compileEnhancements": false,
    "extensions": [
      "ts"
    ],
    "require": [
      "ts-node/register"
    ],
    "files": [
      "src/tests/*.test.ts",
      "src/tests/**/*.test.ts"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hayspec/monorepo.git"
  },
  "bugs": {
    "url": "https://github.com/hayspec/monorepo/issues"
  },
  "homepage": "https://github.com/hayspec/monorepo#readme",
  "keywords": [
    "test",
    "testing",
    "spec",
    "specification",
    "hay",
    "javascript",
    "typescript",
    "nodejs",
    "tdd",
    "unit",
    "cli"
  ],
  "author": "Kristijan Sedlak (Xpepermint)",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^10.3.2",
    "ava": "1.0.0-beta.6",
    "ts-node": "^6.1.0",
    "tslint": "5.10.0",
    "typescript": "^2.9.1"
  },
  "gitHead": "66f3914992286ebc7e3678acd4a2f9e92ab49a10"
}
