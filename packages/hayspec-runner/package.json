{
  "name": "@hayspec/runner",
  "version": "0.4.15",
  "description": "Tests runner for Hayspec framework.",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "clean": "rm -Rf ./dist",
    "transpile": "npm run clean; tsc",
    "lint": "tslint 'src/**/*.ts?(x)'",
    "prepublishOnly": "npm run transpile",
    "test": "ava"
  },
  "ava": {
    "compileEnhancements": false,
    "extensions": [
      "ts"
    ],
    "require": [
      "ts-node/register"
    ],
    "files": [
      "src/tests/*.test.ts",
      "src/tests/**/*.test.ts"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hayspec/monorepo.git"
  },
  "bugs": {
    "url": "https://github.com/hayspec/monorepo/issues"
  },
  "homepage": "https://github.com/hayspec/monorepo#readme",
  "keywords": [
    "test",
    "testing",
    "spec",
    "specification",
    "hay",
    "javascript",
    "typescript",
    "nodejs",
    "tdd",
    "unit",
    "cli"
  ],
  "author": "Kristijan Sedlak (Xpepermint)",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^10.3.3",
    "ava": "^1.0.0-beta.6",
    "ts-node": "^6.1.0",
    "tslint": "^5.10.0",
    "typescript": "^2.9.1"
  },
  "dependencies": {
    "@hayspec/spec": "^0.4.15",
    "fast-glob": "^2.2.2"
  },
  "gitHead": "03535c698ebd35ec94c63869f18cdde2380f739f"
}
