{
  "name": "@hayspec/init",
  "version": "0.8.4",
  "description": "Project generator for Hayspec framework.",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "clean": "rm -Rf ./dist",
    "build": "npm run clean; npx tsc",
    "test": "npx nyc npx ava --verbose"
  },
  "ava": {
    "compileEnhancements": false,
    "extensions": [
      "ts"
    ],
    "require": [
      "ts-node/register"
    ],
    "files": [
      "src/tests/*.test.ts",
      "src/tests/**/*.test.ts"
    ]
  },
  "nyc": {
    "extension": [
      ".ts"
    ],
    "require": [
      "ts-node/register"
    ],
    "exclude": [
      "src/tests"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hayspec/framework.git"
  },
  "bugs": {
    "url": "https://github.com/hayspec/framework/issues"
  },
  "homepage": "https://github.com/hayspec/framework#readme",
  "keywords": [
    "test",
    "testing",
    "spec",
    "specification",
    "hay",
    "javascript",
    "typescript",
    "nodejs",
    "init",
    "initialize"
  ],
  "author": "Kristijan Sedlak (Xpepermint)",
  "license": "MIT",
  "devDependencies": {
    "@types/fs-extra": "^8.0.0",
    "@types/node": "^12.6.2",
    "ava": "1.0.1",
    "nyc": "^14.1.1",
    "ts-node": "^8.3.0",
    "typescript": "^3.5.0"
  },
  "dependencies": {
    "fs-extra": "^8.1.0"
  }
}
